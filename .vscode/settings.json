{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[wxml]": {
    "editor.defaultFormatter": "qiu8310.minapp-vscode"
  },
  "codetour.showMarkers": true,
  "debug.toolBarLocation": "docked",
  "editor.accessibilitySupport": "off",
  // 启用本地括号匹配
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    // 保存时自动添加缺失的依赖
    "source.addMissingImports": true,
    // 让 import 语句按照字母顺序进行排列，可能与prettier冲突：https://github.com/prettier/prettier-vscode/issues/716，引入https://github.com/prettier/prettier-vscode/issues/716#issuecomment-623931202 链接，使得codeActionsOnSave按顺序执行 (失效，不format后自动排序也会失效)
    // "source.organizeImports"
    "source.fixAll.eslint": true
  },
  // "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 5,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrainsMono NF, Monaco, Menlo, 'Courier New', monospace",
  // 连字效果
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  // 自动更改标签
  "editor.linkedEditing": true,
  // 关闭缩略图
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Storage",
        "scope": ["storage"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#C678DD"
        }
      },
      {
        "name": "Keyword Operator",
        "scope": ["keyword.operator"],
        "settings": {
          "foreground": "#C678DD"
        }
      },
      {
        "name": "Support Type",
        "scope": ["support.type"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#C678DD"
        }
      },
      {
        "name": "Entity Name Type",
        "scope": ["entity.name.type"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#98C379"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] JS/TS Property Definition Variable",
        "scope": [
          "meta.definition.property.js variable",
          "meta.definition.property.ts variable",
          "meta.definition.property.jsx variable",
          "meta.definition.property.tsx variable"
        ],
        "settings": {
          "foreground": "#E06C75"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] JS/TS Keyword Control As",
        "scope": [
          "keyword.control.as.js",
          "keyword.control.as.ts",
          "keyword.control.as.jsx",
          "keyword.control.as.tsx"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#C678DD"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] JS/TS Variable Other ReadWrite",
        "scope": [
          "variable.other.readwrite.js",
          "variable.other.readwrite.ts",
          "variable.other.readwrite.tsx"
        ],
        "settings": {
          "foreground": "#D19A66"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] JS/TS 重定义变量",
        "scope": [
          "meta.object-binding-pattern-variable.js variable.other.readwrite.js",
          "meta.object-binding-pattern-variable.jsx variable.other.readwrite.jsx",
          "meta.object-binding-pattern-variable.ts variable.other.readwrite.ts"
        ],
        "settings": {
          "foreground": "#ABB2BF"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] JS/TS new 方法",
        "scope": ["new.expr entity.name.function"],
        "settings": {
          "foreground": "#E5C07B"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] keyword.operator.expression.typeof",
        "scope": ["keyword.operator.expression.typeof"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#C678DD"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] TS/JS Keyword Operator",
        "scope": [
          "source.ts keyword.operator",
          "source.tsx keyword.operator",
          "source.js keyword.operator",
          "source.jsx keyword.operator"
        ],
        "settings": {
          "foreground": "#ABB2BF"
        }
      },
      {
        "name": "[VSCODE-CUSTOM] JS/TS Type Module",
        "scope": "entity.name.type.module.js,entity.name.type.module.ts,entity.name.type.module.jsx,entity.name.type.module.tsx",
        "settings": {
          "foreground": "#C3D3DE"
        }
      }
    ]
  },
  "editor.unicodeHighlight.allowedLocales": {
    "zh-hans": true,
    "zh-hant": true
  },
  // 以像素为单位控制字号。
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "wxml": "html"
  },
  "errorLens.fontStyleItalic": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  // "minapp-vscode.prettier": {
  //   "parser": "html"
  // },
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // 文件嵌套,支持按名称去让文件按照一定规则展开收起
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "index.ts": "${dirname}.tsx,${dirname}.*.scss,style.*.scss,style.scss"
  },
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": false,
  "fileheader.configObj": {
    "autoAdd": false, // 默认开启
    "createHeader": false, // 新建文件自动添加头部注释
    "dateFormat": "YYYY-MM-DD"
  },
  // 插件配置
  "fileheader.customMade": {
    "Author": "Sea",
    "Date": "Do not edit",
    "LastEditTime": "Do not edit",
    "LastEditors": "Sea"
  },
  "files.associations": {
    "*.cjson": "jsonc",
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.tpl": "typescript",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact",
    "*.wxs": "javascript",
    "*.wxss": "css",
    ".npmpackagejsonlintignore": "ignore"
  },
  // 不需要监测改动或者甚至不需要显示在资源管理器内的文件夹、文件排除
  // 不需要显示
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true
  },
  // 不需要监测改动
  "files.watcherExclude": {
    "**/.docusaurus/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "git.openRepositoryInParentFolders": "never", // Highlight the local line if there's no selection range
  "gitlens.advanced.messages": {
    // "suppressShowKeyBindingsNotice": true
  },
  "jest.autoRun": "off",
  // lint
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.theme": "specific",
  "minapp-vscode.disableAutoConfig": true,
  "minapp-vscode.wxmlFormatter": "prettier", //指定格式化工具
  "openInGitHub.github.domain": "git.garena.com", // Custom GitHub domain
  "openInGitHub.remote.branch": "master", // Name of the remote branch
  "openInGitHub.remote.name": "origin", // Name of the remote repository
  "openInGitHub.useLocalBranch": true, // Use the local branch instead of the fixed remote branch
  "openInGitHub.useLocalLine": false,
  "openInGitHub.useLocalRange": true, // Highlight the local selection range, if there's one
  "playwright.reuseBrowser": true,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  // 项目文件路径
  "projectManager.git.baseFolders": ["$home/workspace"],
  "projectManager.sortList": "Recent",
  "search.followSymlinks": false,
  "security.workspace.trust.untrustedFiles": "open",
  "sortJSON.orderOverride": [
    "name",
    "version",
    "main",
    "module",
    "types",
    "typings",
    "files",
    "publishConfig",
    "repository",
    "scripts",
    "prefix",
    "description",
    "body"
  ],
  "sortJSON.orderUnderride": [
    "resolutions",
    "dependencies",
    "devDependencies",
    "peerDependencies",
    "cSpell.enableFiletypes",
    "cSpell.userWords"
  ],
  "svg.preview.mode": "svg",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "todo-tree.filtering.excludeGlobs": ["**/node_modules", "**/.docusaurus"],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#FFEB95",
      "type": "line"
    }
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.tree.showScanModeButton": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.showReleaseNotes": false,
  // 返回normal时切换输入法至英文
  "vim.autoSwitchInputMethod.defaultIM": "com.apple.keylayout.ABC",
  "vim.autoSwitchInputMethod.enable": true,
  "vim.autoSwitchInputMethod.obtainIMCmd": "/opt/homebrew/bin/im-select",
  "vim.autoSwitchInputMethod.switchIMCmd": "/opt/homebrew/bin/im-select {im}",
  // 快速跳转
  "vim.easymotion": true,
  // 在折叠下代码块下上下移动不会自动展开
  "vim.foldfix": true,
  // 下面定义的按键将交由vscode进行处理，而不是vscode-vim插件
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-c": false,
    "<C-f>": false,
    "C-v": false,
    "C-w": false
  },
  "vim.highlightedyank.color": "rgba(230, 97, 89, 0.7)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "white",
  // 搜索设定高亮
  "vim.hlsearch": true,
  "vim.ignorecase": false,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "r"],
      "commands": ["editor.action.rename"]
    },
    // 和 nvim 配置一样
    {
      "before": ["leader", "w"],
      "commands": [":w!"]
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["leader", "x"],
      "commands": [":x!"]
    },
    {
      "after": ["g", "g", "V", "G"],
      "before": ["<c-a>"]
    },
    {
      // 同鼠标hover时展示的效果一样
      "before": ["<leader>", "k"],
      "commands": ["editor.action.showHover"]
    },
    {
      // 展示上一个错误
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      // 展示下一个错误
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "after": ["^"],
      "before": ["H"]
    },
    {
      "after": ["$"],
      "before": ["L"]
    },
    {
      // 快速切换boolean
      "before": ["leader", "i"],
      "commands": ["extension.toggleBool"]
    }
  ],
  "vim.sneak": true,
  // 会影响性能，且会随时改变 workbench.colorCustomizations 所对应的数据，谨慎开启
  "vim.statusBarColorControl": false,
  "vim.statusBarColors.insert": "#5f0000",
  "vim.statusBarColors.normal": "#005f5f",
  "vim.statusBarColors.replace": "#00000",
  "vim.statusBarColors.visual": "#5f00af",
  "vim.statusBarColors.visualblock": "#86592d",
  "vim.statusBarColors.visualline": "#005f5f",
  "vim.surround": true,
  // 优先使用Vim的键位
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vscode-office.openOutline": true,
  "workbench.colorCustomizations": {
    "editorUnnecessaryCode.border": "#59626f",
    "list.inactiveFocusOutline": "#3080F7",
    "panel.background": "#282C34",
    "sideBar.background": "#282C34",
    "statusBar.background": "#282C34",
    "tab.activeBackground": "#4E545E" //活动选项卡的背景色
    // "list.inactiveSelectionBackground": "#d8ca9e" //侧边栏选中未获得焦点颜色
    // "list.inactiveSelectionBackground": "#3080F7" //侧边栏选中未获得焦点颜色
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editor.untitled.hint": "hidden",
  // "js/ts.implicitProjectConfig.checkJs": true
  // "javascript.validate.enable": false
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  // "workbench.startupEditor": "newUntitledFile",
  "workbench.startupEditor": "readme",
  "zenMode.hideActivityBar": false,
  // "zenMode.hideTabs": false,
  // "window.zoomLevel": 1,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "cSpell.enableFiletypes": ["wxml"],
  "cSpell.userWords": [
    "ahooks",
    "antd",
    "browserslist",
    "cjson",
    "codetour",
    "commitlint",
    "databoard",
    "Diffable",
    "dingtalk",
    "docgen",
    "dotfiles",
    "easymotion",
    "emtransit",
    "errexp",
    "esbenp",
    "execa",
    "fileheader",
    "foldfix",
    "garena",
    "GOOGLEPLUS",
    "Hailang",
    "healthcheck",
    "highlightedyank",
    "hlsearch",
    "Hocs",
    "Hotspot",
    "hotspots",
    "iframe",
    "ignorecase",
    "immer",
    "jsapi",
    "keylayout",
    "lutra",
    "mdast",
    "Menlo",
    "metalsmith",
    "microfe",
    "Microsite",
    "micrositepage",
    "minapp",
    "mobilemall",
    "npmjs",
    "npmpackagejsonlintignore",
    "nprogress",
    "nvim",
    "outfile",
    "pagebuilder",
    "pcmall",
    "pcrw",
    "Pinterest",
    "pnpm",
    "precommit",
    "prettierrc",
    "qiankun",
    "RCMD",
    "readwrite",
    "reduxjs",
    "removeprefix",
    "rfmd",
    "rgba",
    "rmcd",
    "searc",
