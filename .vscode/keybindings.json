  // Open terminal in directory of currently opened file
  // https://stackoverflow.com/a/58311671/9065011
  {
    "key": "cmd+shift+h",
    "command": "workbench.action.terminal.newWithCwd",
    "args": {
      "cwd": "${fileDirname}"
    }
  },
  {
    "key": "cmd+o",
    "command": "projectManager.listProjectsNewWindow"
  }
]
